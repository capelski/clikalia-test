{
    "compilerOptions": {
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "module": "commonjs",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "outDir": "dist",
        "strictNullChecks": true,
        "target": "es5"
    },
    "exclude": ["node_modules/"],
    "include": ["src"]
}
